generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

enum Role {
  admin
  user
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
  role     Role
  
  @@map("users")
}